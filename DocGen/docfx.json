{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "public/Nitrocid/Nitrocid.csproj",
                        "public/Nitrocid.LocaleGen.Core/Nitrocid.LocaleGen.Core.csproj"
                    ],
                    "src": ".."
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "filter": "filterConfig.yml"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "index.md",
                    "toc.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [ 
                    "images/**",
                    "logo.svg",
                    "favicon.ico"
                ]
            }
        ],
        "dest": "../docs",
        "globalMetadata": {
            "_appTitle": "Nitrocid KS API - Aptivi",
            "_enableSearch": true
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [ "default", "theme" ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "sitemap": {
            "baseUrl": "https://aptivi.github.io/NitrocidKS/"
        }
    }
}
