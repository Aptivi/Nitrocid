<Project Sdk="Microsoft.NET.Sdk">
  <!-- KS.Rkm General Information -->
  <PropertyGroup>
    <DebugSymbols>true</DebugSymbols>
    <OutputType>Exe</OutputType>
    <AssemblyName>Nitrocid.Rkm</AssemblyName>
    <AssemblyTitle>Nitrocid KS - Remote Kernel Manager</AssemblyTitle>
    <Description>Simulates our future-planned kernel, Nitrocid Kernel - Remote Kernel Manager</Description>
    <OutputPath>../../Nitrocid/KSBuild/</OutputPath>
    <ApplicationIcon>../../Nitrocid/OfficialAppIcon-NitrocidKS.ico</ApplicationIcon>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <!-- KS.Rkm General Information End -->

  <!-- KS.Rkm Dependencies Information -->
  <ItemGroup>
    <PackageReference Include="Aptivestigate.Serilog" Version="1.3.2" PrivateAssets="all" />
  </ItemGroup>
  <!-- KS.Rkm Dependencies Information End -->

  <!-- KS.Rkm Non-private Dependencies Information -->
  <ItemGroup>
    <PackageReference Include="Terminaux" Version="8.0.7" PrivateAssets="contentfiles;analyzers;build;runtime;native" />
    <PackageReference Include="Terminaux.Images" Version="8.0.7" PrivateAssets="contentfiles;analyzers;build;runtime;native" />
  </ItemGroup>
  <!-- KS.Rkm Non-private Dependencies Information End -->

  <!-- KS.Rkm Locale Checker -->
  <ItemGroup>
    <ProjectReference Include="..\..\..\private\Nitrocid.LocaleChecker\Nitrocid.LocaleChecker.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
    <ProjectReference Include="..\..\Nitrocid.Base\Nitrocid.Base.csproj" />
    <ProjectReference Include="..\..\Nitrocid.Core\Nitrocid.Core.csproj" />
  </ItemGroup>
  <!-- KS.Rkm Locale Checker End -->
</Project>
