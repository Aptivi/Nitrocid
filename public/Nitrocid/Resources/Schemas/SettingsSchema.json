{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Settings Schema",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "Name": {
                "type": "string"
            },
            "Desc": {
                "type": "string"
            },
            "DisplayAs": {
                "type": "string"
            },
            "Keys": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "Name": {
                            "type": "string"
                        },
                        "Type": {
                            "type": "string"
                        },
                        "Variable": {
                            "type": "string"
                        },
                        "Description": {
                            "type": "string"
                        },
                        "Masked": {
                            "type": "boolean"
                        },
                        "IsEnumeration": {
                            "type": "boolean"
                        },
                        "SelectionFunctionName": {
                            "type": "string"
                        },
                        "SelectionFunctionType": {
                            "type": "string"
                        },
                        "IsSelectionFunctionDict": {
                            "type": "boolean"
                        },
                        "SelectionFallback": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "EnumerationInternal": {
                            "type": "boolean"
                        },
                        "Enumeration": {
                            "type": "string"
                        },
                        "EnumerationAssembly": {
                            "type": "string"
                        },
                        "EnumerationZeroBased": {
                            "type": "boolean"
                        },
                        "UnsupportedPlatforms": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "IsValuePath": {
                            "type": "boolean"
                        },
                        "IsPathCurrentPath": {
                            "type": "boolean"
                        },
                        "DelimiterVariable": {
                            "type": "string"
                        },
                        "ShellType": {
                            "type": "string"
                        },
                        "MinimumValue": {
                            "type": "number"
                        },
                        "MaximumValue": {
                            "type": "number"
                        },
                        "Delimiter": {
                            "type": "string"
                        },
                        "ValuePathType": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "Name",
                        "Type"
                    ]
                }
            }
        },
        "required": [
            "Name",
            "Desc",
            "Keys"
        ]
    }
}
